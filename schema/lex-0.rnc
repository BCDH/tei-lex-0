  }
gramGrp =
  [
    a:documentation [
      "(grammatical information group) groups morpho-syntactic information about a lexical item, e.g. "
      ns1:code [ "<pos>" ]
      ", "
      ns1:code [ "<gen>" ]
      ", "
      ns1:code [ "<number>" ]
      ", "
      ns1:code [ "<case>" ]
      ", or "
      ns1:code [ "<iType>" ]
      " (inflectional class). [10.3.2. Grammatical Information]"
    ]
  ]
  element gramGrp {
    (text
     | model.gLike
     | model.lexPhrase
     | model.lexInter
     | gramGrp
     | usg
     | model.morphLike
     | model.global)*,
    att.global.attributes,
    att.lexicographic.attributes,
    att.typed.attributes,
    empty
  }
